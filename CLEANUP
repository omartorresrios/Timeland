#/bin/sh

echo "Removing files generated by Cocoapods"
rm -rf Pods/!ProtoCompiler  Pods/!ProtoCompiler-gRPCPlugin Pods/BoringSSL Pods/gRPC Pods/gRPC-Core Pods/gRPC-ProtoRPC Pods/gRPC-RxLibrary Pods/Local\ Podspecs/googleapis.podspec.json Pods/Protobuf Pods/Target\ Support\ Files/BoringSSL Pods/Target\ Support\ Files/googleapis Pods/Target\ Support\ Files/gRPC Pods/Target\ Support\ Files/gRPC-Core Pods/Target\ Support\ Files/gRPC-ProtoRPC Pods/Target\ Support\ Files/gRPC-RxLibrary Pods/Target\ Support\ Files/Pods-Speech Pods/Target\ Support\ Files/Protobuf

echo "Removing files generated by the protocol buffer compiler"
find google -name "*.[hm]" -exec rm {} \; 
